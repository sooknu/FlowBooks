{
  "name": "web-app",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "vite --host :: --port 3000",
    "dev:server": "tsx watch --env-file=.env server/index.ts",
    "build": "node tools/generate-llms.js || true && vite build",
    "deploy": "npm run build && pm2 restart FlowBooks-server",
    "start": "tsx --env-file=.env server/index.ts",
    "preview": "vite preview --host :: --port 3000",
    "db:push": "drizzle-kit push",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:generate": "drizzle-kit generate",
    "migrate:items": "tsx --env-file=.env server/scripts/migrate-items-to-tables.ts",
    "start:worker": "tsx --env-file=.env server/worker.ts",
    "dev:worker": "tsx watch --env-file=.env server/worker.ts",
    "restore": "node scripts/restore.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.991.0",
    "@better-auth/passkey": "^1.4.18",
    "@emotion/is-prop-valid": "^1.3.1",
    "@fastify/compress": "^8.3.1",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.2.0",
    "@fastify/etag": "^6.1.0",
    "@fastify/helmet": "^13.0.2",
    "@fastify/multipart": "^9.4.0",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/static": "^9.0.0",
    "@paypal/paypal-server-sdk": "^2.2.0",
    "@paypal/react-paypal-js": "^8.9.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.3",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@stripe/react-stripe-js": "^5.6.0",
    "@stripe/stripe-js": "^8.7.0",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-query-devtools": "^5.91.3",
    "better-auth": "^1.4.18",
    "bullmq": "^5.67.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cmdk": "^0.2.0",
    "concurrently": "^9.2.1",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.45.1",
    "fastify": "^5.7.4",
    "framer-motion": "^10.16.4",
    "googleapis": "^171.4.0",
    "ioredis": "^5.9.2",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.285.0",
    "nodemailer": "^8.0.0",
    "pdf-lib": "^1.17.1",
    "postgres": "^3.4.8",
    "react": "^18.2.0",
